<h1>New picture</h1>



<% form_for(@picture) do |f| %>
  <%= f.error_messages %>



  <p>
    <%= f.label :name rescue nil%><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :url rescue nil%><br />
    <%= f.text_field :url %>
  </p>
  <p>
    <%= f.label :rating rescue nil%><br />
    <%= f.text_field :rating %>
  </p>
  <p>
    <%= f.label :user_id rescue nil%><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', :controller => "home", :action => "index" %>